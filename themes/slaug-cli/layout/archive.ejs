<%
var title;

if (is_archive()){
    title = "Projects";

    if (is_month()){
    title += ': ' + page.year + '/' + page.month;
    } else if (is_year()){
    title += ': ' + page.year;
    }
} else if (is_category()){
    title = 'Category: ' + page.category;
} else if (is_tag()){
    title = 'Tag: ' + page.tag;
}
%>
<h1 class="centered"><%- title %></h1>
<%- partial('_partial/archive', {expand: false, index: true}) %>